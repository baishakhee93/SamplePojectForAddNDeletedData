<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<!-- Background -->
<item>
    <shape
        android:shape="rectangle">
        <solid android:color="#FFFFFF" />
        <corners android:radius="20dp" /> <!-- Adjust the radius to control the roundness -->
    </shape>
</item>



<item
    android:left="0dp"
    android:right="0dp"
    android:top="0dp"
    android:bottom="0dp">
    <shape
        android:shape="rectangle">
        <stroke
            android:color="#CCCCCC"
            android:width="1dp" />
        <corners android:radius="20dp" />
    </shape>
</item>


</layer-list>
